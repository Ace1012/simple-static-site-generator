{
  "version": 2,
  "builds": [{
    "src": ".dist/server.js",
    "use": "@vercel/node"
  }],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "statusCode": 404,
      "destination": "dist/templates/error/404.html"
    }
  ]
}
